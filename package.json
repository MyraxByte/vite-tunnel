{
	"name": "@vite-libs/tunnel",
	"type": "module",
	"version": "1.0.8",
	"description": "A vite plugin for tunneling",
	"author": "Anton Ryuben (https://github.com/MyraxByte)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MyraxByte/vite-tunnel.git"
	},
	"bugs": "https://github.com/MyraxByte/vite-tunnel/issues",
	"license": "MIT",
	"keywords": [
		"vite-plugin",
		"vite-plugin-tunnel",
		"vite-tunnel",
		"vite"
	],
	"exports": {
		".": {
			"types": "./dist/plugin.d.ts",
			"import": "./dist/plugin.mjs",
			"require": "./dist/plugin.cjs"
		},
		"./*": "./*"
	},
	"main": "dist/plugin.cjs",
	"module": "dist/plugin.mjs",
	"types": "dist/plugin.d.ts",
	"files": [
		"*.d.ts",
		"dist",
		"**/*.js"
	],
	"scripts": {
		"build": "unbuild --minify",
		"stub": "unbuild --stub"
	},
	"peerDependencies": {
		"vite": "^3.1.0 || ^4.0.0-0 || ^5.0.0-0"
	},
	"dependencies": {
		"@vite-libs/devtools": "^1.0.8",
		"untun": "^0.1.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.5",
		"unbuild": "^2.0.0"
	}
}
