{
	"name": "@vite-libs/tunnel",
	"type": "module",
	"version": "1.0.1",
	"description": "A vite plugin for tunneling",
	"author": "Anton Ryuben",
	"license": "MIT",
	"keywords": [
		"vite-plugin",
		"vite-plugin-tunnel",
		"vite-tunnel",
		"vite"
	],
	"exports": {
		".": {
			"types": "./dist/plugin/index.d.ts",
			"import": "./dist/plugin/index.mjs",
			"require": "./dist/plugin/index.cjs"
		},
		"./*": "./*"
	},
	"main": "dist/plugin/index.cjs",
	"module": "dist/plugin/index.mjs",
	"types": "dist/plugin/index.d.ts",
	"files": [
		"*.d.ts",
		"dist"
	],
	"scripts": {
		"build": "unbuild --minify",
		"stub": "unbuild --stub"
	},
	"peerDependencies": {
		"vite": "^3.1.0 || ^4.0.0-0 || ^5.0.0-0"
	},
	"dependencies": {
		"@vite-libs/devtools": "^1.0.1",
		"untun": "^0.1.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.5",
		"unbuild": "^2.0.0"
	}
}