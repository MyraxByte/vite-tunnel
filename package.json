{
	"name": "vite-tunnel",
	"type": "module",
	"version": "1.0.0",
	"description": "A vite plugin for tunneling",
	"author": "Anton Ryuben",
	"license": "MIT",
	"keywords": [
	  "vite-plugin",
	  "vite-plugin-tunnel",
	  "vite-tunnel"
	],
	"exports": {
	  ".": {
		"types": "./dist/plugin.d.ts",
		"import": "./dist/plugin.mjs",
		"require": "./dist/plugin.cjs"
	  },
	  "./*": "./*"
	},
	"main": "dist/plugin.cjs",
	"module": "dist/plugin.mjs",
	"types": "dist/plugin.d.ts",
	"files": [
	  "*.d.ts",
	  "./src/devtools.js",
	  "./src/devtools/**",
	  "dist",
	  "devtools"
	],
	"scripts": {
	  "build": "unbuild",
	  "stub": "unbuild --stub"
	},
	"peerDependencies": {
	  "vite": "^3.1.0 || ^4.0.0-0 || ^5.0.0-0"
	},
	"dependencies": {
	  "untun": "^0.1.3"
	},
	"devDependencies": {
	  "@types/node": "^20.11.5",
	  "unbuild": "^2.0.0"
	}
  }